#!/bin/sh

passes=~/term/new/build/deb/pass/
llvmtools=~/term/llvm/build/Release+Asserts/bin/
llvmgcc=~/term/llvm-gcc-bin/bin/

llvmgcc=$llvmgcc"x86_64-unknown-linux-gnu-llvm-g++"
llvmopt=$llvmtools"opt"
llvmas=$llvmtools"llvm-as"

$llvmgcc -O2 -S -emit-llvm test.cpp
$llvmas test.s -f
$llvmopt -load $passes"libhelloWorld.so" -hello < test.s.bc > /dev/null
   
